<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/library_navigation"
    app:progressDestination="@+id/frameworkProgressFragment"
    app:startDestination="@id/library_list_fragment">

    <fragment
        android:id="@+id/library_list_fragment"
        android:name="com.sorrowblue.comicviewer.library.LibraryListFragment"
        android:label="ライブラリ"
        app:bottom_nav_visible="true">
        <action
            android:id="@+id/action_library_list_to_dropbox_navigation"
            app:destination="@id/dropbox_navigation" />
        <action
            android:id="@+id/action_library_list_to_googledrive_navigation"
            app:destination="@id/googledrive" />
        <action
            android:id="@+id/action_library_list_to_box_navigation"
            app:destination="@id/box_navigation" />
        <action
            android:id="@+id/action_library_list_to_onedrive_navigation"
            app:destination="@id/onedrive" />
    </fragment>
    <include app:graph="@navigation/dropbox" />
    <include-dynamic
        android:id="@+id/googledrive"
        app:graphPackage="${applicationId}.googledrive"
        app:graphResName="googledrive"
        app:moduleName="googledrive" />
    <include-dynamic
        android:id="@+id/onedrive"
        app:graphPackage="${applicationId}.onedrive"
        app:graphResName="onedrive"
        app:moduleName="onedrive" />
    <include app:graph="@navigation/box" />
    <fragment
        android:id="@+id/frameworkProgressFragment"
        android:name="com.sorrowblue.comicviewer.framework.ui.fragment.FrameworkProgressFragment"
        android:label="FrameworkProgressFragment" />
</navigation>
