<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/server_management_navigation_device"
    app:startDestination="@id/server_management_device_fragment">

    <framework-fragment
        android:id="@+id/server_management_device_fragment"
        android:name="com.sorrowblue.comicviewer.server.management.device.ServerManagementDeviceFragment"
        android:label="ローカルストレージの登録・編集">
        <argument
            android:name="bookshelf_id"
            app:argType="integer"
            android:defaultValue="-1" />
        <argument
            android:name="transition_name"
            android:defaultValue="@null"
            app:argType="string"
            app:nullable="true" />
    </framework-fragment>
</navigation>
